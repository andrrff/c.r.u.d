<!DOCTYPE html>
<html lang="en">
<head>
    <%- include ../partials/head %>
</head>
<body class="container">

<header>
    <%- include ../partials/header %>
</header>

<main>
    <div class="jumbotron">
        
        
        <ul>
            <% data.forEach(function(index){ %>
                <p><%= index.id %></p>
                <p><%= index.name %></p>
                <p><%= index.preco %></p>
                <p style='margin-bottom: 5pc;'><%= index.descricao %></p>
            <% }); %>
        </ul>
        
    </div>
</main>

<footer>
    <%- include ../partials/footer %>
</footer>

</body>
</html>