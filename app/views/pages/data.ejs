<!DOCTYPE html>
<html lang="en">
<head>
    <%- include ../partials/head %>
</head>
<body class="container">

<header>
    <%- include ../partials/header %>
</header>

<main>
    <div class="jumbotron">
          <form class="d-flex">
                <div class="input-group mb-3">
                    <input type="username" class="form-control" name="search-nickname" placeholder="üöß in work üöß" aria-label="Recipient's username" aria-describedby="button-addon2" disabled>
                    <input class="btn btn-outline-primary"  type="submit" id="button-addon2" disabled>
                </div>
            </form>
            
        <% data.forEach(function(index){ %>
                <div class="list-group" style='margin-bottom: 5pc;'>
                    <button onclick="window.location.href='./data/<%= index.id %>'" type="button" class="list-group-item list-group-item-action active" aria-current="true">
                        /api/data/<%= index.id %>
                    </button>
                    <button type="button" class="list-group-item list-group-item-action" disabled>Primeiro nome: <%= index.firstname %></button>
                    <button type="button" class="list-group-item list-group-item-action" disabled>Segundo nome: <%= index.lastname %></button>
                    <button type="button" class="list-group-item list-group-item-action" disabled>Nickname: <%= index.nickname %></button>
                    <button type="button" class="list-group-item list-group-item-action" disabled>Endere√ßo: <%= index.address %></button>
                    <button type="button" class="list-group-item list-group-item-action" disabled>Bio: <%= index.bio %></button>
                    <button type="button" class="list-group-item list-group-item-action" disabled>Data de cria√ß√£o: <%= index.dataLancamento %></button>
                </div>
            <% }); %>        
    </div>
</main>

<footer>
    <%- include ../partials/footer %>
</footer>

</body>
</html>